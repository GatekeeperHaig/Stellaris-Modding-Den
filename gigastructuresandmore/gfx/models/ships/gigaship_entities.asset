entity = {
	name = "gigahugeship_full_entity"
	cull_radius = 1000
	
	# Here I refer to position values as x,y,z from left to right
	# x,z are on the planetary plane, y is height
	# x = cos(rotation)*radius, y = 0, z = sin(rotation)*radius
	# eg. loc103 
	# cos(30) = 0.866 , sin(30) = 0.5
	# x =  250*0.866 = 216.5 , y = 0 , z = 250*0.5 = 125
	# rotate the ringworld chunk 30 degree iterations
	# planet plane ring			
	
	locator = { name = "loc101" position = { -5 0 0 } rotation = { 90 90 0 }}
	locator = { name = "loc102" position = { 5 0 0 } rotation = { -90 -90 0 }}
	locator = { name = "loc103" position = { 0 0 0.5 } rotation = { 0 0 90 }}

		
	default_state = "idle"
	state = { name = "idle" animation = "idle" }

	attach = { "loc101" = "hugeship_side_entity"}
	attach = { "loc102" = "hugeship_side_entity"}
	attach = { "loc103" = "hugeship_front_entity"}

	
	scale = 1 # change to increase or decrease distance between stars and rings, also effects planetary ring!
}

