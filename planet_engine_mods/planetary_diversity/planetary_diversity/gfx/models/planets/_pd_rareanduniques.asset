entity = {
    name = "pd_petrified_planet_01_entity"
    pdxmesh = "planet_clouded_mesh"
     
    meshsettings = {
    name = "planet_geosphereShape"	
    texture_diffuse = "petrified_01_diffuse.dds"
	texture_normal = "petrified_01_normals.dds"
	texture_specular = "petrified_01_spec.dds"
