entity = {
	name = "plantoid_01_orbital_station_entity"
	pdxmesh = "plantoid_01_orbital_station_frame_mesh"	
	scale = 1.4

	locator = { name = "part8" 		position = { 0 0 0 } }
	locator = { name = "part9" 		position = { 0 0 0 } }
	locator = { name = "part10" 		position = { 0 0 0 } }	
	locator = { name = "light_pos_1"	position = {  4 13   0 } }	
	locator = { name = "light_pos_2"	position = { -4 13   0 } }	
	locator = { name = "light_pos_3"	position = {  0 13   4 } }	
	locator = { name = "light_pos_4"	position = {  0 13  -4 } }	
	
	default_state = "idle" 
	state = { name = "idle" state_time = 5 
		event = { trigger_once = yes sound = { soundeffect = "spaceport_hum_idle" }  } 
		start_event = { node = "light_pos_1" light = "planetoid_light" }
		start_event = { node = "light_pos_2" light = "planetoid_light" }
		start_event = { node = "light_pos_3" light = "planetoid_light" }
		start_event = { node = "light_pos_4" light = "planetoid_light" }
	}
	state = { name = "death" state_time = 5 looping = no
		event = { time = 4.9 particle = "large_station_explosion_particle" keep_particle = yes trigger_once = yes sound = { soundeffect = "station_explosion_large" } }
	}

	game_data = {
		emissive_recolor_crunch = 4.0
	}
}

