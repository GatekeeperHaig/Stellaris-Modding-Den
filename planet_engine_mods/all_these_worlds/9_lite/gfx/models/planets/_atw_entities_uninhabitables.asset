## EXOTIC CLASSES (for single-class uninhabitable worlds)

# Molten Class - uses vanilla molten_planet entity

# Icy Class
entity = {
	name = "icy_gd_planet_01_entity"
	pdxmesh = "planet_no_pole_no_clouds_mesh"
	
	meshsettings = {
	name = "planet_geosphereShape"
	texture_diffuse = "_gd_icy_01_diffuse.dds"
	texture_normal = "_gd_icy_01_normal.dds"
	texture_specular = "_gd_icy_01_specular.dds"
	}
	
	default_state = "idle"
	state = { name = "idle" animation = "idle" time_offset = { 0 100 } 
		start_event = { trigger_once = yes sound = { soundeffect = "amb_planet_class" } }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_planet_class_02" } }
	}
}
