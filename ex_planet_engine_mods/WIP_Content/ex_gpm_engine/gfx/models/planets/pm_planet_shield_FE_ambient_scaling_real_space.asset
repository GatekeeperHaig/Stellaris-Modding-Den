entity = {
	name = "pm_planet_shield_FE_RS_scaling_25_ambient_entity"
	pdxmesh = "pm_planet_shield_ambient_mesh"
	
	meshsettings = {
		name = "planet_geosphereShape"
		texture_diffuse = "pm_planet_shield_diffuse_fe.dds"
		texture_normal = "nonormal.dds"
		texture_specular = "nospec.dds"
		shader = "PdxMeshExtraDimensionalShip"
	}
	
	##pole texture override
	meshsettings = {
		name = "polesShape"
		texture_diffuse = "pm_planet_shield_diffuse_fe.dds"  #pm_planet_shield_diffuse_fe.dds
		texture_normal = "nonormal.dds"
		texture_specular = "nospec.dds"
		shader = "PdxMeshExtraDimensionalShip"
	}
	
	##cloud texture override
	meshsettings = {
		name = "clouds_geosphereShape"
		texture_diffuse = "pm_planet_shield_clouds.dds"
		texture_normal = "pm_planet_shield_clouds.dds"
		texture_specular = "nospec.dds"
	}
	
	##cloud shadow texture override
	meshsettings = {
		name = "clouds_shadow_geosphereShape"
		texture_diffuse = "pm_planet_shield_clouds.dds"
		texture_normal = "nonormal.dds"
		texture_specular = "nospec.dds"
	}

	default_state = "idle"
	state = { name = "idle" animation = "idle" time_offset = { 0 500 }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_planet_class" } }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_planet_class_02" } }
	}
		
	scale = 16.5
}
